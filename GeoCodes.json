{
    "swagger": "2.0",
    "info": {
        "title": "Google",
        "description": "API for Google GeoCoding",
        "version": "0.0.0"
    },
    "host": "maps.googleapis.com",
    "basePath": "/maps/api/geocode/",
    "schemes": [
        "https"
    ],
    "paths": {
        "/json": {
            "post": {
                "tags": [
                    "GeoCode"
                ],
                "description": "Convert address to a geocode",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "key",
                        "in": "query",
                        "description": "API key",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "address",
                        "in": "query",
                        "description": "the address to code",
                        "required": true,
                        "type": "string",
                        "x-example": "111 main street, new york, ny"
                    },
                    {
                        "name": "components",
                        "in": "query",
                        "description": "components used for further filtering delimited by |.  Do not repeat items already submitted in address!",
                        "required": false,
                        "type": "string",
                        "x-example": "country:USA"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/CodeResponse"
                        },
                        "examples":{
                            "application/json":{

                            }
                        }
                    }
                }
            },
            "security": []
        }
    },
    "definitions": {
        "CodeResponse": {
            "description": "Model for GeoCodeRespons",
            "properties": {
                "results": [{
                    "type": "array",
                    "items": {
                        "properties":{
                            "address_components": [{
                                "type": "array",
                                "$ref": "#/definitions/AddressComponents"
                            }],
                            "formatted_address": {
                                "type": "string"
                            },
                            "geometry":{
                                "type": "object",
                                "properties": {
                                    "bounds": {
                                        "type": "object",
                                        "properties":{
                                            "northeast": {
                                                "type": "object",
                                                "properties":{
                                                    "lat":{
                                                        "type": "number"
                                                    },
                                                    "lng": {
                                                        "type": "number"
                                                    }
                                                }
                                            },
                                            "southwest": {
                                                "type": "object",
                                                "properties":{
                                                    "lat":{
                                                        "type": "number"
                                                    },
                                                    "lng": {
                                                        "type": "number"
                                                    }
                                                } 
                                            }
                                        }
                                    },
                                    "location": {
                                        "type": "object",
                                        "properties": {
                                            "lat":{
                                                "type": "number"
                                            },
                                            "lng": {
                                                "type": "number"
                                            }
                                        }
                                    },
                                    "location_type": {
                                        "type": "string"
                                    },
                                    "viewport":{
                                        "type": "object",
                                        "properties": {
                                            "northeast": {
                                                "type": "object",
                                                "properties":{
                                                    "lat":{
                                                        "type": "number"
                                                    },
                                                    "lng": {
                                                        "type": "number"
                                                    }
                                                }
                                            },
                                            "southwest": {
                                                "type": "object",
                                                "properties":{
                                                    "lat":{
                                                        "type": "number"
                                                    },
                                                    "lng": {
                                                        "type": "number"
                                                    }
                                                } 
                                            }
                                        }
                                    }
                                }
                            },
                            "place_id": {
                                "type": "string"
                            },
                            "postcode_localities": [{
                                "type": "array",
                                "items": {
                                    "type": "string"
                                }
                            }],
                            "types": {
                                "type": "array",
                                "items": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }],
                "status": {
                    "type": "string"
                }

            }
        },
        "AddressComponents":{
            "description": "Model for the Adress Components",
            "type": "object",
            "properties":{
                "long_name":{
                    "type": "string"
                },
                "short_name": {
                    "type": "string"
                },
                "types": [{
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                }]
            }

        }
        
    },
    "securityDefinitions": {},
    "security": []
}
